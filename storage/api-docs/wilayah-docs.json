                                                    "nama_kabupaten": {
                                                        "type": "string",
                                                        "example": "Kab. Kepulauan Seribu"
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "example": "2023-12-05T13:19:05.000000Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "example": "2023-12-05T13:19:05.000000Z"
                                                    }
                                                }
                                            },
                                            "example": [
