    "paths": {
        "/province": {
            "get": {
                "tags": ["Province"],
                "description": "get all group whatshapp",
                "responses": {
                    "200": {
                        "description": "success fetch data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "integer",
                                                        "format": "int32",
                                                        "example": "1"
                                                    },
                                                    "kode_provinsi": {
                                                        "type": "integer",
                                                        "format": "int32",
                                                        "example": "10000"
                                                    },
                                                    "nama_provinsi": {
                                                        "type": "string",
                                                        "example": "Prov. D.K.I. Jakarta"
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "example": "2023-12-05T13:25:33.000000Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "example": "2023-12-05T13:25:33.000000Z"
                                                    }
                                                }
                                            },
                                            "example": [
                                                {
                                                    "id": 1,
                                                    "kode_provinsi": 10000,
                                                    "nama_provinsi": "Prov. D.K.I. Jakarta",
                                                    "created_at": "2023-12-05T13:25:33.000000Z",
                                                    "updated_at": "2023-12-05T13:25:33.000000Z"
                                                }
                                            ]
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "success fetch data"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "format": "int32",
                                            "example": "200"
                                        },
                                        "status": {
                                            "type": "boolean"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
